{
  "name": "postcss-font-awesome",
  "version": "0.5.0",
  "description": "PostCSS plugin to pull in a Font Awesome's icon unicode",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "font awesome"
  ],
  "author": "Eric Pfeiffer <eric@marketingrelevance.com>",
  "license": "MIT",
  "repository": "https://github.com/marketing-relevance/postcss-font-awesome",
  "bugs": {
    "url": "https://github.com/marketing-relevance/postcss-font-awesome/issues"
  },
  "homepage": "https://github.com/marketing-relevance/postcss-font-awesome",
  "dependencies": {
    "postcss": "^7.0.0"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "eslint": "^7.11.0",
    "eslint-config-postcss": "^4.0.0"
  },
  "scripts": {
    "test": "ava && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es6",
    "rules": {
      "max-len": [
        1,
        80,
        4
      ]
    }
  },
  "main": "index.js"
}
